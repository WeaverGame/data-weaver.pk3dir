models/mapobjects/light_props/candleflame
{
	qer_editorImage		models/mapobjects/light_props/candleflame.png
	xmap_surfaceLight	500
	xmap_lightSubdivide	16
	xmap_lightImage		models/mapobjects/light_props/candleflame.png
	xmap_lightRGB 1 0.9 0.7
	twosided
	nonsolid
//	deformvertexes autosprite2
	{
		stage		glowMap
		map		models/mapobjects/light_props/candleflame.png
		blend		add
	}
}

models/mapobjects/light_props/chainlink
{
	qer_editorImage		models/mapobjects/light_props/chainlink.png
	twosided
	noShadows
