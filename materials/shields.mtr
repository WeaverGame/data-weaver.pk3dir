textures/shields/shield
{
	qer_editorimage    models/entities/shield.png

	{
		map			models/entities/shield.png
		rgb			0.1 0.1 0.1
		rotate		(time * -0.8)
		scroll		0.7, 0.7
		blend		add
		scale		2, 2
		alpha		0.2
	}
}

models/entities/shield_info
{
	qer_editorimage    models/entities/shield_info.png
	
	{
		map         models/entities/shield_info.png
		blend blend
		//blendFunc GL_SRC_ALPHA GL_ONE_MINUS_SRC_ALPHA
	}
}